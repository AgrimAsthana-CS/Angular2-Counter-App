{
  "compilerOptions": {
    "module": "system",
    "moduleResolution":"node",
    "target": "ES5",
    "outDir":"compiled",
    "sourceMap": true,
    "emitDecoratorMetadata":true,
    "experimentalDecorators":true,
    "removeComments":true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules"
  ]
}